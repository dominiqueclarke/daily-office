{
   "background": {
      "persistent": false,
      "scripts": [ "moment.min.js", "moment-range.min.js", "advent.js", "background.js" ]
   },
   "browser_action": {
     "default_icon": "icon.png"
   },
   "description": "The most beautiful daily bible verse plugin.  Select a reading plans, read the verse whenever you open a new tab.",
   "key": "",
   "manifest_version": 2,
   "name": "Daily Office Liturgy Calendar",
   "permissions": ["tabs", "http://d3js.org/d3.v3.min.js", "https://ajax.googleapis.com/", "http://www.esvapi.org/", "http://calapi.inadiutorium.cz/"],
   "version": "1.0",
   "web_accessible_resources": [ "reading_plan.js", "jqmeter.min.js", "circle-progress.js" ]
}
